<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>


<HBox alignment="CENTER" styleClass="navbar-bg" stylesheets="@style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.autoeliteproject.navbar2Controller">
   <children>
      <Button fx:id="ourjourneyBtn" mnemonicParsing="false" onAction="#handleOurJourneyButtonClick" styleClass="about-us-button" stylesheets="@style.css" text="Our Journey">
         <HBox.margin>
            <Insets right="50.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="meetteamBtn" mnemonicParsing="false" onAction="#handleMeetTeamButtonClick" styleClass="about-us-button" stylesheets="@style.css" text="Meet The Team">
         <HBox.margin>
            <Insets right="50.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="joinusBtn" mnemonicParsing="false" onAction="#handleJoinUsButtonClick" styleClass="about-us-button" stylesheets="@style.css" text="Join Us">
         <HBox.margin>
            <Insets right="50.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
