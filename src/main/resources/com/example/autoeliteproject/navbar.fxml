<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" spacing="20.0" styleClass="nav-bar" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.autoeliteproject.navbarController">
   <children>
      <ImageView fitHeight="72.0" fitWidth="92.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets bottom="5.0" top="5.0" />
         </HBox.margin>
         <image>
            <Image url="@images/Asset%204.png" />
         </image>
      </ImageView>
      <Button fx:id="homeBtn" mnemonicParsing="false" onAction="#handleHomeButtonClick" styleClass="about-us-button" stylesheets="@style.css" text="Home">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
      <Button fx:id="aboutusBtn" mnemonicParsing="false" onAction="#handleAboutUsButtonClick" styleClass="about-us-button" stylesheets="@style.css" text="About Us">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
      <Button fx:id="viewCarsBtn" mnemonicParsing="false" onAction="#handleViewCarsButtonClick" styleClass="about-us-button" stylesheets="@style.css" text="View Cars">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
      <Button fx:id="contactsBtn" mnemonicParsing="false" onAction="#handleContactsButtonClick" styleClass="about-us-button" stylesheets="@style.css" text="Contacts" />
   </children>
</HBox>
